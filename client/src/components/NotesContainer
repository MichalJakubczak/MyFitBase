import React from 'react';
import Wrapper from '../assets/wrappers/NotesContainer';
import { useNotesContext } from '../pages/AllNotes';


const NotesContainer = () => {
    const { data } = useNotesContext();
    const { notes } = data;
  
    if (notes.length === 0) {
      return (
        <Wrapper>
      )
        
    
}
    return (
        <Wrapper>
          <div className='notes'>
            {notes.map((note) => {
              return <Note key={note.id} {...note} />
            })}
          </div>
        </Wrapper>
      );
    };

export default NotesContainer;

